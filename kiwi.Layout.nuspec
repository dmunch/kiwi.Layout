        <file src="kiwi.Layout.iOS/bin/iPhone/Debug/kiwi.Layout.iOS.dll" target="lib/xamarinios"/>
	
	<file src="kiwi.Layout/bin/Debug/kiwi.Layout.dll" target="lib/portable-net45+win+wp80+MonoAndroid10+MonoTouch10"/>
